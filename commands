
find ~/vm-manager -type f \( -name "*.py" -o -name "*.html" \) -print0 \
| while IFS= read -r -d '' file; do
    echo "===== $file =====" >> all_code.txt
    cat "$file" >> all_code.txt
    echo -e "\n\n" >> all_code.txt
done





